<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="ViewFlow">
        <attr name="sidebuffer" format="integer" />
    </declare-styleable>
    <declare-styleable name="ViewFlowCircleIndicator">
        <attr name="activeColor" format="color" />
        <attr name="inactiveColor" format="color" />
        <attr name="radius" format="dimension" />
        <attr name="centered" format="boolean" />
        <attr name="fadeOut" format="integer" />
        <attr name="inactiveType">
            <flag name="stroke" value="0" />
            <flag name="fill" value="1" />
        </attr>
        <attr name="activeType">
            <flag name="stroke" value="0" />
            <flag name="fill" value="1" />
        </attr>
        <attr name="circleSeparation" format="dimension" />
        <attr name="activeRadius" format="dimension" />
    </declare-styleable>
    <declare-styleable name="CustomTextView">
        <attr name="ctv_boundWidth" format="dimension" />
        <attr name="ctv_boundHeight" format="dimension" />
        <attr name="ctv_drawableCenter" format="boolean" />
        <attr name="android:text" />
        <attr name="ctv_subText" format="string" />
        <attr name="android:textSize" />
        <attr name="ctv_subTextSize" format="dimension" />
        <attr name="android:textColor" />
        <attr name="ctv_subTextColor" format="color" />
        <attr name="android:checkMark" />
        <attr name="ctv_checkMarkBound" format="dimension" />
    </declare-styleable>
    <declare-styleable name="slideswitch">
        <attr name="onThemeImage" format="reference" />
        <attr name="offThemeImage" format="reference" />
        <attr name="onCircleImage" format="reference" />
        <attr name="offCircleImage" format="reference" />
        <attr name="onThemeColor" format="color" />
        <attr name="offThemeColor" format="color" />
        <attr name="onCircleColor" format="color" />
        <attr name="offCircleColor" format="color" />
        <attr name="slideable" format="boolean" />
        <attr name="isOpen" format="boolean" />
        <attr name="shape">
            <enum name="rect" value="1" />
            <enum name="circle" value="2" />
        </attr>
    </declare-styleable>
    <declare-styleable name="CustomRadioButton">
        <attr name="crb_boundWidth" format="dimension"/>
        <attr name="crb_boundHeight" format="dimension"/>
        <attr name="crb_subText" format="string"/>
        <attr name="crb_subTextSize" format="dimension"/>
        <attr name="crb_subTextColor" format="color"/>
        <attr name="android:text" />
        <attr name="android:textSize" />
        <attr name="android:textColor" />
    </declare-styleable>
    <declare-styleable name="CustomRadioGroup">
        <attr name="android:checkedButton" />
        <attr name="android:orientation" />
    </declare-styleable>
    <declare-styleable name="PagerSlidingTabStrip">
        <attr name="pstsIndicatorColor" format="color" />
        <attr name="pstsUnderlineColor" format="color" />
        <attr name="pstsDividerColor" format="color" />
        <attr name="pstsSelectedTabTextColor" format="color" />
        <attr name="pstsIndicatorHeight" format="dimension" />
        <attr name="pstsUnderlineHeight" format="dimension" />
        <attr name="pstsDividerPadding" format="dimension" />
        <attr name="pstsTabPaddingLeftRight" format="dimension" />
        <attr name="pstsScrollOffset" format="dimension" />
        <attr name="pstsTabBackground" format="reference" />
        <attr name="pstsShouldExpand" format="boolean" />
        <attr name="pstsTextAllCaps" format="boolean" />
        <attr name="pstsMatrix" format="boolean" />
    </declare-styleable>
    <declare-styleable name="TitleIndicator">
        <attr name="tid_TitlePadding" format="dimension" />
        <!-- Left/right padding of not active view titles. -->
        <attr name="tid_clipPadding" format="dimension" />
        <attr name="tid_textColor" format="color|reference" />
        <attr name="tid_textSizeNormal" format="dimension" />
        <attr name="tid_textSizeSelected" format="dimension" />
        <attr name="tid_flagLineHeight" format="dimension" />
        <attr name="tid_flagLineColor" format="color" />
        <attr name="tid_flagLineTriangleHeight" format="dimension" />
        <attr name="tid_flagOrientation">
            <flag name="top" value="0" />
            <flag name="bottom" value="1" />
        </attr>
        <attr name="tid_compoundOrientation">
            <flag name="top" value="0" />
            <flag name="bottom" value="1" />
            <flag name="left" value="2" />
            <flag name="right" value="3" />
        </attr>
        <attr name="tid_compundBound" format="dimension" />
        <attr name="tid_focusDrawable" format="reference"/>
        <attr name="tid_unfocusDrawable" format="reference"/>
        <attr name="tid_selectedIndex" format="integer" />
        <attr name="tid_textArray" format="string" />
    </declare-styleable>
    <declare-styleable name="EcoGallery">
        <attr name="gravity" format="integer" />
        <attr name="animationDuration" format="integer" />
        <attr name="actionDistance" format="integer" />
        <attr name="unselectedAlpha" format="float" />
        <attr name="unselectedScale" format="float" />
        <attr name="scaleDownGravity" format="float" />
        <attr name="spacing" format="dimension" />
        <attr name="slideOnePage" format="boolean" />
    </declare-styleable>

    <attr name="ecoGalleryStyle" format="reference" />

    <declare-styleable name="CustomAbsSpinner">
        <attr name="entries" format="reference" />
    </declare-styleable>

    <attr name="customAbsSpinnerStyle" format="reference" />

    <declare-styleable name="CircleIndicator">

        <!-- Whether or not the indicators should be centered. -->
        <attr name="centered" />
        <!-- Color of the filled circle that represents the current page. -->
        <attr name="fillColor" format="color" />
        <!-- Color of the filled circles that represents pages. -->
        <attr name="pageColor" format="color" />
        <!-- Orientation of the indicator. -->
        <attr name="android:orientation" />
        <!-- Radius of the circles. This is also the spacing between circles. -->
        <attr name="radius" />
        <!-- Whether or not the selected indicator snaps to the circles. -->
        <attr name="snap" format="boolean" />
        <!-- Color of the open circles. -->
        <attr name="strokeColor" format="color" />
        <!-- Width of the stroke used to draw the circles. -->
        <attr name="strokeWidth" format="dimension" />
        <!-- View background -->
        <attr name="android:background" />
    </declare-styleable>
    <declare-styleable name="FancyCoverFlow">
        <attr name="unselectedAlpha" />
        <attr name="unselectedSaturation" format="float" />
        <attr name="unselectedScale" />
        <attr name="maxRotation" format="integer" />
        <attr name="scaleDownGravity" />
        <attr name="actionDistance" />
    </declare-styleable>
    <declare-styleable name="switchButton">
        <attr name="android:textColor" />
        <attr name="android:textSize" />
        <attr name="sw_textArray" format="string" />
        <attr name="sw_switchCount" format="integer" />
        <attr name="sw_themeStyle" format="reference" />
        <attr name="sw_cornerRadius" format="dimension" />
        <attr name="sw_checkedColor" format="color|reference" />
        <attr name="sw_unCheckedColor" format="color|reference" />
        <attr name="sw_strokeColor" format="color|reference" />
        <attr name="sw_strokeWidth" format="dimension" />
    </declare-styleable>

    <!-- used to set the minTextSize via xml -->
    <attr name="autoScaleTextViewStyle" format="reference" />

    <declare-styleable name="AutoScaleTextView">
        <attr name="minTextSize" format="dimension" />
    </declare-styleable>

    <declare-styleable name="SecureInputView">
        <attr name="borderWidth" format="dimension" />
        <attr name="borderColor" format="color" />
        <attr name="borderColorFocus" format="color" />
        <attr name="borderRadius" format="dimension" />
        <attr name="passwordWidth" format="dimension" />
        <attr name="passwordColor" format="color" />
        <attr name="passwordRadius" format="dimension" />
        <attr name="displayPassWord" format="boolean" />
        <attr name="android:maxLength"/>
    </declare-styleable>
</resources>